{
  "_base": "./client",

  "address": "",
  "port": 8080,

  "filters": [ "DefaultGuard" ],

  "routes": {
    "post:/verifySession": { "handler": "MockAccountModule.postVerify", "allow": [] },
    "get:/verifySession": { "handler": "MockAccountModule.getVerify", "allow": [] },
    "*:/verifySession": { "handler": "MockSessionModule.verify", "allow": [] },
    
    "get:/test": { "handler": "NoModule.test", "allow": [] },
    "get:/test2": { "handler": "MockSessionModule.nomethod", "allow": [] },
    
    "get:/image/*": { "handler": "MockDefaultModule.compress" },
    
    "post:*": { "handler": "MockDefaultModule.post" },
    "get:*": { "handler": "MockDefaultModule.default" },
    "get:*": { "handler": "MockDefaultModule.raw" }
  }
}